{
  "name": "custom/leafletjs",
  "type": "drupal-module",
  "description": "Interactive Leaflet map with marker clustering. Displays a leaflet map using a preprocessed CSV file with location data to prevent long query times for large data sets.",
  "keywords": [
    "Drupal",
    "leaflet",
    "map",
    "clustering",
    "markers",
    "geolocation"
  ],
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/Kxka/drupal_leafletjs",
  "support": {
    "issues": "https://github.com/Kxka/drupal_leafletjs/issues",
    "source": "https://github.com/Kxka/drupal_leafletjs"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "drupal/core": "^9 || ^10 || ^11",
    "drupal/views_data_export": "^1.8",
    "drupal/views_geojson": "^1.4",
    "drupal/geolocation": "^3.14"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\leafletjs\\": "src/"
    }
  }
}
